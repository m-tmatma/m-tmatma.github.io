#: ch06-server-configuration.xml:88
#, no-c-format
msgid "Authentication options"
msgstr ""

#. Tag: entry
#: ch06-server-configuration.xml:89
#, no-c-format
msgid ""
"HTTP(S) basic auth, X.509 certificates, LDAP, NTLM, or any other mechanism "
"available to Apache httpd"
msgstr ""

